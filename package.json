{
	"name": "ccf-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"tsc": "tsc",
		"clean": "rimraf build",
		"build": "npm run clean && npm run tsc --",
		"lint": "tslint src/**/*.ts",
		"watch": "npm run build -- -w",
		"emu": "nohup ~/Library/Android/sdk/tools/emulator -avd Nexus_6P_API_23 &",
		"ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
		"android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'"
	},
	"dependencies": {
		"react": "16.2.0",
		"react-native": "0.53.0"
	},
	"devDependencies": {
		"@types/jest": "^22.1.2",
		"@types/react": "^16.0.36",
		"@types/react-native": "^0.52.8",
		"babel-jest": "22.2.2",
		"babel-preset-react-native": "4.0.0",
		"concurrently": "^3.5.1",
		"jest": "22.2.2",
		"react-native-cli": "^2.0.1",
		"react-test-renderer": "16.2.0",
		"rimraf": "^2.6.2",
		"tslint": "^5.9.1",
		"typescript": "^2.7.1"
	},
	"jest": {
		"preset": "react-native"
	}
}

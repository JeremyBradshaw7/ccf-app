{
	"name": "ccfapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest",
		"tsc": "tsc",
		"clean": "rimraf build",
		"build": "npm run clean && npm run tsc --",
		"lint": "tslint src/**/*.ts",
		"watch": "npm run build -- -w",
		"emu": "nohup ~/Library/Android/sdk/tools/emulator -avd Nexus_6P_API_23 &",
		"ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
		"android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
		"all": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios' 'react-native run-android'"
	},
	"dependencies": {
		"native-base": "^2.3.9",
		"react": "16.2.0",
		"react-native": "0.53.0",
		"react-navigation": "^1.0.3",
		"react-navigation-slide-from-right-transition": "^1.0.4",
		"react-redux": "^5.0.6",
		"redux": "^3.7.2",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0"
	},
	"devDependencies": {
		"@types/jest": "^22.1.2",
		"@types/react": "^16.0.38",
		"@types/react-dom": "^16.0.4",
		"@types/react-native": "^0.52.8",
		"@types/react-redux": "^5.0.15",
		"babel-jest": "22.2.2",
		"babel-preset-react-native": "4.0.0",
		"concurrently": "^3.5.1",
		"jest": "22.2.2",
		"react-native-cli": "^2.0.1",
		"react-test-renderer": "16.2.0",
		"rimraf": "^2.6.2",
		"tslint": "^5.9.1",
		"typescript": "^2.7.1",
		"utility-types": "^1.0.0"
	},
	"jest": {
		"preset": "react-native"
	}
}
